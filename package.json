    "deepmerge": "^2.1.1",
    "echarts": "^4.1.0",
    "element-ui": "^2.4.3",
    "exportjs": "^0.2.2",
    "quill": "^1.3.6",
    "quill-delta": "^3.6.2",
    "sortablejs": "^1.7.0",
    "vue": "^2.5.16",
